        c4 = c_gray; 
        c5 = c_gray;
        c6 = c_green;   
        if(screen == 1) {
            text = "Start your game";  
        }
    }    
}

// Loops the positions of the menu
if(position &lt; 1) {
    position = max_option;
} else if(position &gt; max_option) {
    position = 1;
}

